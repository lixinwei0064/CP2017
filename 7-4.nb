Notebook[{
Cell[BoxData[{
 RowBox[{"Clear", "[", "\"\<Global`*\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mesh", "[", 
   RowBox[{
   "x1_", ",", "x2_", ",", "hx_", ",", "y1_", ",", "y2_", ",", "hy_"}], "]"}],
   ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"x", ",", "x1", ",", "x2", ",", "hx"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"y", ",", "y1", ",", "y2", ",", "hy"}], "}"}]}], "]"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mesh", "::", "usage"}], "=", 
  "\"\<Build the matrix in the given rectangular area\>\""}], "\
\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"randomWalk", "[", 
   RowBox[{
   "x1_", ",", "x2_", ",", "hx_", ",", "y1_", ",", "y2_", ",", "hy_", ",", 
    "n_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "a", ",", "numx", ",", "numy", ",", "x", ",", "y", ",", "lenx", ",", 
      "leny", ",", "maxx", ",", "maxy", ",", "i", ",", 
      RowBox[{"b", "=", 
       RowBox[{"{", "}"}]}], ",", "rand"}], "}"}], ",", 
    RowBox[{
     RowBox[{"a", "=", 
      RowBox[{"mesh", "[", 
       RowBox[{"x1", ",", "x2", ",", "hx", ",", "y1", ",", "y2", ",", "hy"}], 
       "]"}]}], ";", 
     RowBox[{"lenx", "=", 
      RowBox[{"Length", "[", "a", "]"}]}], ";", 
     RowBox[{"leny", "=", 
      RowBox[{"Length", "[", 
       RowBox[{"a", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"maxx", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"a", "[", 
         RowBox[{"[", "lenx", "]"}], "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", 
     RowBox[{"maxy", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"a", "[", 
         RowBox[{"[", "1", "]"}], "]"}], "[", 
        RowBox[{"[", "leny", "]"}], "]"}], "[", 
       RowBox[{"[", "2", "]"}], "]"}]}], ";", 
     RowBox[{"numx", "=", 
      RowBox[{"RandomInteger", "[", 
       RowBox[{"{", 
        RowBox[{"1", ",", "lenx"}], "}"}], "]"}]}], ";", 
     RowBox[{"numy", "=", 
      RowBox[{"RandomInteger", "[", 
       RowBox[{"{", 
        RowBox[{"1", ",", "leny"}], "}"}], "]"}]}], ";", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"x", ",", "y"}], "}"}], "=", 
      RowBox[{
       RowBox[{"a", "[", 
        RowBox[{"[", "numx", "]"}], "]"}], "[", 
       RowBox[{"[", "numy", "]"}], "]"}]}], ";", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"b", ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}]}], "]"}], ";", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "0"}], ",", 
       RowBox[{"i", "<", "n"}], ",", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"b", ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", "y"}], "}"}]}], "]"}], ";", 
        RowBox[{"i", "++"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"Which", "[", "\[IndentingNewLine]", 
         RowBox[{"(*", 
          RowBox[{"Point", " ", "at", " ", "the", " ", "corner"}], "*)"}], 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], "==", 
           RowBox[{"{", 
            RowBox[{"x1", ",", "maxy"}], "}"}]}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "1", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "==", "0"}], ",", 
             RowBox[{"numx", "++"}], ",", 
             RowBox[{"rand", "==", "1"}], ",", 
             RowBox[{"numy", "--"}]}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], "==", 
           RowBox[{"{", 
            RowBox[{"x1", ",", "y1"}], "}"}]}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "1", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "==", "0"}], ",", 
             RowBox[{"numx", "++"}], ",", 
             RowBox[{"rand", "==", "1"}], ",", 
             RowBox[{"numy", "++"}]}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], "==", 
           RowBox[{"{", 
            RowBox[{"maxx", ",", "y1"}], "}"}]}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "1", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "==", "0"}], ",", 
             RowBox[{"numx", "--"}], ",", 
             RowBox[{"rand", "==", "1"}], ",", 
             RowBox[{"numy", "++"}]}], "]"}]}], ",", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"x", ",", "y"}], "}"}], "==", 
           RowBox[{"{", 
            RowBox[{"maxx", ",", "maxy"}], "}"}]}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "1", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "==", "0"}], ",", 
             RowBox[{"numx", "--"}], ",", 
             RowBox[{"rand", "==", "1"}], ",", 
             RowBox[{"numy", "--"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{"Point", " ", "on", " ", "four", " ", "sides"}], "*)"}], 
          "\[IndentingNewLine]", 
          RowBox[{"x", "\[Equal]", "x1"}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "2", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "\[Equal]", "0"}], ",", 
             RowBox[{"numx", "++"}], ",", 
             RowBox[{"rand", "\[Equal]", "1"}], ",", 
             RowBox[{"numy", "++"}], ",", 
             RowBox[{"rand", "\[Equal]", "2"}], ",", 
             RowBox[{"numy", "--"}]}], "]"}]}], ",", 
          RowBox[{"x", "\[Equal]", "maxx"}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "2", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "\[Equal]", "0"}], ",", 
             RowBox[{"numx", "--"}], ",", 
             RowBox[{"rand", "\[Equal]", "1"}], ",", 
             RowBox[{"numy", "++"}], ",", 
             RowBox[{"rand", "\[Equal]", "2"}], ",", 
             RowBox[{"numy", "--"}]}], "]"}]}], ",", 
          RowBox[{"y", "\[Equal]", "y1"}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "2", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "\[Equal]", "0"}], ",", 
             RowBox[{"numy", "++"}], ",", 
             RowBox[{"rand", "\[Equal]", "1"}], ",", 
             RowBox[{"numx", "++"}], ",", 
             RowBox[{"rand", "\[Equal]", "2"}], ",", 
             RowBox[{"numx", "--"}]}], "]"}]}], ",", 
          RowBox[{"y", "\[Equal]", "maxy"}], ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "2", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "\[Equal]", "0"}], ",", 
             RowBox[{"numy", "--"}], ",", 
             RowBox[{"rand", "\[Equal]", "1"}], ",", 
             RowBox[{"numx", "++"}], ",", 
             RowBox[{"rand", "\[Equal]", "2"}], ",", 
             RowBox[{"numx", "--"}]}], "]"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"(*", 
           RowBox[{"Point", " ", "inside"}], "*)"}], "\[IndentingNewLine]", 
          "True", ",", 
          RowBox[{
           RowBox[{"rand", "=", 
            RowBox[{"RandomInteger", "[", "3", "]"}]}], ";", 
           RowBox[{"Which", "[", 
            RowBox[{
             RowBox[{"rand", "\[Equal]", "0"}], ",", 
             RowBox[{"numx", "--"}], ",", 
             RowBox[{"rand", "\[Equal]", "1"}], ",", 
             RowBox[{"numx", "++"}], ",", 
             RowBox[{"rand", "\[Equal]", "2"}], ",", 
             RowBox[{"numy", "--"}], ",", 
             RowBox[{"rand", "\[Equal]", "3"}], ",", 
             RowBox[{"numy", "++"}]}], "]"}]}]}], "\[IndentingNewLine]", 
         "]"}], ";", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"x", ",", "y"}], "}"}], "=", 
         RowBox[{
          RowBox[{"a", "[", 
           RowBox[{"[", "numx", "]"}], "]"}], "[", 
          RowBox[{"[", "numy", "]"}], "]"}]}]}]}], "\[IndentingNewLine]", 
      "]"}], ";", 
     RowBox[{"Show", "[", 
